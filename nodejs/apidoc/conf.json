{
    "opts": {
        "recurse": true,
        "template": "config/jsdoc/api/template"
    },
    "tags": {
        "allowUnknownTags": true
    },
    "source": {
        "includePattern": ".+\\.js(doc)?$",
        "excludePattern": "(^|\\/|\\\\)_",
        "include": []
    },
    "plugins": [
        "plugins/markdown"
    ],
    "markdown": {
        "parser": "gfm"
    },
    "templates": {
        "cleverLinks": true,
        "monospaceLinks": true,
        "default": {
            "outputSourceFiles": false
        },
        "applicationName": "uGisMapPlatForm"
    },
    "jsVersion": 180
}
